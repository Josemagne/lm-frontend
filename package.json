    "formik": "^2.2.9",
    "gsap": "^3.10.4",
    "localpersistence": "^1.2.8",
    "nanoid": "^3.3.1",
    "netlify-cli": "^9.13.0",
    "node": "^17.4.0",
    "os-browserify": "^0.3.0",
    "postcss-loader": "^6.2.1",
    "react": "^17.0.2",
    "react-beautiful-dnd": "^13.1.0",
